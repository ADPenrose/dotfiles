{
  "Export Statements for React": {
    "prefix": "esr",
    "body": [
      "export * from \"./${1:COMPONENT}\";",
      "export { default } from \"./${1:COMPONENT}\";"
    ]
  },
  "ESlint Vars/Props": {
    "prefix": "esvp",
    "body": ["\"no-unused-vars\": \"warn\",", "\"react/prop-types\": \"off\""]
  }
}
